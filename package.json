    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/nconf": "^0.0.37",
    "@types/node": "^10.11.7",
    "@types/std-mocks": "^1.0.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.0",
    "mocha": "^5.2.0",
    "release-it": "^10.1.0",
    "rimraf": "^2.6.2",
    "std-mocks": "^1.0.1",
    "supertest": "^3.4.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.2",
    "typescript-eslint-parser": "^20.0.0"
  },
  "files": [
    "src",
    "LICENSE",
    "README.md"
